        // Initialize on page load
        document.addEventListener('DOMContentLoaded', function() {
            createAppCards();
            setupCategoryFiltering();
            setupSearch();
            setupAgeTabs();
            setupFAQToggle();
            
            // Add slight delay to app cards appearance for a staggered animation effect
            const appCards = document.querySelectorAll('.app-card');
            appCards.forEach((card, index) => {
                card.style.animationDelay = `${index * 0.05}s`;
                card.style.opacity = '0';
                setTimeout(() => {
                    card.style.opacity = '1';
                }, 100 + (index * 50));
            });
            
            const debugInfo = document.getElementById('debugInfo');
            debugInfo.innerHTML = "DOM loaded successfully";
        });
        
        // Function to create a nice name from filename (from original index.html)
        function getNiceName(filename) {
            // Remove file extension
            let name = filename.replace(".html", "");
            
            // Remove version numbers
            name = name.replace(/v\d+(\.\d+)?/ig, "").replace(/V\d+(\.\d+)?/ig, "");
            
            // Replace hyphens, underscores and dots with spaces
            name = name.replace(/[-_.]/g, " ");
            
            // Capitalize first letter of each word
            name = name.split(" ").map(word => {
                if (word.length > 0) {
                    return word[0].toUpperCase() + word.slice(1);
                }
                return word;
            }).join(" ");
            
            // Trim any extra spaces
            name = name.trim();
            
            return name;
        }
        
        // Function to get icon for app based on filename (from original index.html)
        function getIconForApp(filename) {
            const lowerName = filename.toLowerCase();
            
            if (lowerName.includes("math") || lowerName.includes("multiply") || 
                lowerName.includes("division") || lowerName.includes("fraction") || 
                lowerName.includes("algebra") || lowerName.includes("calculus") || 
                lowerName.includes("factor")) {
                return "fas fa-calculator";
            } else if (lowerName.includes("music") || lowerName.includes("piano") || 
                    lowerName.includes("midi") || lowerName.includes("note") || 
                    lowerName.includes("fifths")) {
                return "fas fa-music";
            } else if (lowerName.includes("country") || lowerName.includes("geo") || 
                    lowerName.includes("globe")) {
                return "fas fa-globe-americas";
            } else if (lowerName.includes("game") || lowerName.includes("battleship") || 
                    lowerName.includes("pacman") || lowerName.includes("shooter") || 
                    lowerName.includes("connect4") || lowerName.includes("soduku")) {
                return "fas fa-gamepad";
            } else if (lowerName.includes("read") || lowerName.includes("writing") || 
                    lowerName.includes("word")) {
                return "fas fa-book";
            } else if (lowerName.includes("timer")) {
                return "fas fa-clock";
            } else if (lowerName.includes("neural") || lowerName.includes("ai")) {
                return "fas fa-brain";
            } else {
                return "fas fa-puzzle-piece"; // Default icon
            }
        }
        
        // Function to get categories for an app based on filename (from original index.html)
        function getCategoriesForApp(filename) {
            const lowerName = filename.toLowerCase();
            const categories = [];
            
            // Add categories based on keywords in the filename
            if (lowerName.includes("math") || lowerName.includes("multiply") || 
                lowerName.includes("division") || lowerName.includes("fraction") || 
                lowerName.includes("algebra") || lowerName.includes("calculus") || 
                lowerName.includes("factor") || lowerName.includes("geometry") ||
                lowerName.includes("number") || lowerName.includes("pemdas") ||
                lowerName.includes("subtract") || lowerName.includes("scientific") ||
                lowerName.includes("zero") || lowerName.includes("plot") ||
                lowerName.includes("triang") || lowerName.includes("trig") ||
                lowerName.includes("decimal") || lowerName.includes("statistics")) {
                categories.push("math");
            }
            
            if (lowerName.includes("music") || lowerName.includes("piano") || 
                lowerName.includes("midi") || lowerName.includes("note") || 
                lowerName.includes("fifths") || lowerName.includes("harmonic")) {
                categories.push("music");
            }
            
            if (lowerName.includes("country") || lowerName.includes("geo") || 
                lowerName.includes("globe")) {
                categories.push("geography");
            }
            
            if (lowerName.includes("reading") || lowerName.includes("writing") || 
                lowerName.includes("word")) {
                categories.push("language");
            }
            
            if (lowerName.includes("game") || lowerName.includes("battleship") || 
                lowerName.includes("pacman") || lowerName.includes("shooter") || 
                lowerName.includes("connect4") || lowerName.includes("adventure") ||
                lowerName.includes("soduku") || lowerName.includes("bubble") ||
                lowerName.includes("asteroid") || lowerName.includes("snake") ||
                lowerName.includes("solitaire") || lowerName.includes("trouble") ||
                lowerName.includes("lander") || lowerName.includes("mahjong")) {
                categories.push("games");
            }
            
            // Add "all" category to every app
            categories.push("all");
            
            return categories;
        }
        
        // Create app cards for each file
        function createAppCards() {
            const toddlerGrid = document.getElementById('toddlerGrid');
            const elementaryGrid = document.getElementById('elementaryGrid');
            
            // Function to create an app card
            function createCard(filename, parentFolder) {
                const niceName = getNiceName(filename);
                const iconClass = getIconForApp(filename);
                const categories = getCategoriesForApp(filename);
                
                const card = document.createElement('a');
                card.href = `${parentFolder}/${filename}`;
                card.className = 'app-card';
                card.dataset.categories = categories.join(" ");
                
                const primaryCategory = categories[0] !== "all" ? categories[0] : "interactive";
                
                card.innerHTML = `
                    <div class="app-icon">
                        <i class="${iconClass}"></i>
                    </div>
                    <div class="app-info">
                        <div class="app-name">${niceName}</div>
                        <div class="app-category"><i class="fas fa-tag"></i> ${primaryCategory}</div>
                    </div>
                `;
                
                return card;
            }
            
            // Create cards for toddler apps
            repository["2 yr old"].forEach((filename, index) => {
                const card = createCard(filename, "2 yr old");
                card.style.animationDelay = `${index * 0.05}s`;
                toddlerGrid.appendChild(card);
            });
            
            // Create cards for elementary apps
            repository["5 yr old"].forEach((filename, index) => {
                const card = createCard(filename, "5 yr old");
                card.style.animationDelay = `${index * 0.02}s`;
                elementaryGrid.appendChild(card);
            });
        }
        
        // Handle category filtering
        function setupCategoryFiltering() {
            const categories = document.querySelectorAll('.category');
            const appCards = document.querySelectorAll('.app-card');
            
            categories.forEach(category => {
                category.addEventListener('click', () => {
                    // Update active state
                    categories.forEach(cat => cat.classList.remove('active'));
                    category.classList.add('active');
                    
                    const selectedCategory = category.dataset.category;
                    
                    // Filter the app cards
                    appCards.forEach(card => {
                        const cardCategories = card.dataset.categories.split(" ");
                        
                        if (selectedCategory === 'all' || cardCategories.includes(selectedCategory)) {
                            card.style.display = 'flex';
                        } else {
                            card.style.display = 'none';
                        }
                    });
                });
            });
        }
        
        // Handle search functionality
        function setupSearch() {
            const searchBox = document.getElementById('searchBox');
            const appCards = document.querySelectorAll('.app-card');
            
            searchBox.addEventListener('input', () => {
                const searchTerm = searchBox.value.toLowerCase();
                
                appCards.forEach(card => {
                    const appName = card.querySelector('.app-name').textContent.toLowerCase();
                    
                    if (appName.includes(searchTerm)) {
                        card.style.display = 'flex';
                    } else {
                        card.style.display = 'none';
                    }
                });
            });
        }
        
        // Setup age tabs
        function setupAgeTabs() {
            const ageTabs = document.querySelectorAll('.age-tab');
            const toddlerApps = document.getElementById('toddler-apps');
            const elementaryApps = document.getElementById('elementary-apps');
            
            ageTabs.forEach(tab => {
                tab.addEventListener('click', () => {
                    // Update active state
                    ageTabs.forEach(t => t.classList.remove('active'));
                    tab.classList.add('active');
                    
                    const selectedAge = tab.dataset.age;
                    
                    // Show/hide appropriate section
                    if (selectedAge === 'toddler') {
                        toddlerApps.style.display = 'block';
                        elementaryApps.style.display = 'none';
                    } else {
                        toddlerApps.style.display = 'none';
                        elementaryApps.style.display = 'block';
                    }
                });
            });
        }
        
        // Setup FAQ toggle
        function setupFAQToggle() {
            const faqQuestions = document.querySelectorAll('.faq-question');
            
            faqQuestions.forEach(question => {
                question.addEventListener('click', () => {
                    const answer = question.nextElementSibling;
                    const icon = question.querySelector('i');
                    
                    // Toggle the answer visibility
                    if (answer.style.display === 'none' || !answer.style.display) {
                        answer.style.display = 'block';
                        icon.classList.remove('fa-chevron-down');
                        icon.classList.add('fa-chevron-up');
