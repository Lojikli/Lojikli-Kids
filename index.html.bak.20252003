        // Wait for DOM to be fully loaded
        document.addEventListener('DOMContentLoaded', function() {
            // Initialize components
            initNavigation();
            generateApps();
            initAgeGroupButtons();
            initCategoryFilter();
            initSearch();
            
            // Hide loading spinner after a short delay
            setTimeout(function() {
                document.getElementById('spinner').classList.add('hidden');
            }, 800);
        });
        
        // Initialize navigation functionality
        function initNavigation() {
            const hamburger = document.getElementById('hamburger');
            const mobileNav = document.getElementById('mobile-nav');
            const mobileClose = document.getElementById('mobile-close');
            const overlay = document.getElementById('overlay');
            const mobileLinks = document.querySelectorAll('.mobile-link');
            
            // Toggle mobile navigation
            hamburger.addEventListener('click', function() {
                mobileNav.classList.add('active');
                overlay.classList.add('active');
                document.body.style.overflow = 'hidden';
            });
            
            // Close mobile navigation
            function closeNav() {
                mobileNav.classList.remove('active');
                overlay.classList.remove('active');
                document.body.style.overflow = '';
            }
            
            mobileClose.addEventListener('click', closeNav);
            overlay.addEventListener('click', closeNav);
            
            // Close navigation when clicking on mobile links
            mobileLinks.forEach(link => {
                link.addEventListener('click', closeNav);
            });
            
            // Smooth scroll for navigation links
            const links = document.querySelectorAll('a[href^="#"]');
            links.forEach(link => {
                link.addEventListener('click', function(e) {
                    const href = this.getAttribute('href');
                    
                    if (href === '#') return;
                    
                    e.preventDefault();
                    
                    const offsetTop = document.querySelector(href).offsetTop - 80;
                    
                    scroll({
                        top: offsetTop,
                        behavior: 'smooth'
                    });
                });
            });
        }
        
        // Generate app cards for all apps
        function generateApps() {
            const allAppsGrid = document.getElementById('all-apps');
            const toddlerAppsGrid = document.getElementById('toddler-apps');
            const elementaryAppsGrid = document.getElementById('elementary-apps');
            const upperAppsGrid = document.getElementById('upper-apps');
            
            // Clear existing app cards
            allAppsGrid.innerHTML = '';
            toddlerAppsGrid.innerHTML = '';
            elementaryAppsGrid.innerHTML = '';
            upperAppsGrid.innerHTML = '';
            
            // Combine all apps for the "all apps" view
            const allApps = [
                ...appRepository.toddler,
                ...appRepository.elementary,
                ...appRepository.upper
            ];
            
            // Generate app cards
            generateAppCards(allApps, allAppsGrid);
            generateAppCards(appRepository.toddler, toddlerAppsGrid);
            generateAppCards(appRepository.elementary, elementaryAppsGrid);
            generateAppCards(appRepository.upper, upperAppsGrid);
            
            // Create additional cards based on originalRepository
            createOriginalAppCards();
        }
        
        // Generate app cards for a specific list of apps
        function generateAppCards(apps, container) {
            apps.forEach(app => {
                const appCard = document.createElement('div');
                appCard.className = 'app-card';
                appCard.dataset.category = app.category;
                
                appCard.innerHTML = `
                    <div class="app-image">
                        <i class="${app.icon}"></i>
                    </div>
                    <div class="app-content">
                        <h3 class="app-title">${app.name}</h3>
                        <p class="app-description">${app.description}</p>
                        <div class="app-meta">
                            <span class="app-category-label">${app.category}</span>
                            <span class="app-age-label">${app.age}</span>
                        </div>
                    </div>
                    <div class="app-card-footer">
                        <div class="app-rating">
                            <i class="fas fa-star"></i>
                            <span>${app.rating}</span>
                        </div>
                        <a href="#" class="app-button">Play Now</a>
                    </div>
                `;
                
                container.appendChild(appCard);
            });
        }
        
