    <script>
        // Initialize Three.js Background
        function initThreeJsBackground() {
            const canvas = document.getElementById('hero-canvas');
            const renderer = new THREE.WebGLRenderer({ canvas, alpha: true, antialias: true });
            renderer.setSize(window.innerWidth, window.innerHeight);
            
            const scene = new THREE.Scene();
            const camera = new THREE.PerspectiveCamera(75, window.innerWidth / window.innerHeight, 0.1, 1000);
            camera.position.z = 5;
            
            // Create floating educational elements
            const objectsCount = 15;
            const objects = [];
            const colors = [0x4361ee, 0xff9e00, 0x00c896, 0x9381ff, 0xf72585];
            const geometries = [
                new THREE.TorusGeometry(0.3, 0.1, 16, 25),
                new THREE.IcosahedronGeometry(0.3, 0),
                new THREE.TetrahedronGeometry(0.3, 0),
                new THREE.BoxGeometry(0.4, 0.4, 0.4),
                new THREE.ConeGeometry(0.3, 0.6, 8)
            ];
            
            for (let i = 0; i < objectsCount; i++) {
                const geometry = geometries[Math.floor(Math.random() * geometries.length)];
                const color = colors[Math.floor(Math.random() * colors.length)];
                const material = new THREE.MeshBasicMaterial({ color, wireframe: true });
                const object = new THREE.Mesh(geometry, material);
                
                // Random position spanning the entire scene width but more concentrated at the sides
                const widthMultiplier = Math.random() < 0.5 ? -1 : 1;
                const xRange = Math.random() * 10 + 3;
                
                object.position.x = widthMultiplier * xRange;
                object.position.y = (Math.random() - 0.5) * 8;
                object.position.z = (Math.random() - 0.5) * 5 - 2;
                
                // Add random rotation
                object.rotation.x = Math.random() * Math.PI;
                object.rotation.y = Math.random() * Math.PI;
                
                // Add random rotation animation
                object.rotationSpeed = {
                    x: (Math.random() - 0.5) * 0.01,
                    y: (Math.random() - 0.5) * 0.01,
                    z: (Math.random() - 0.5) * 0.01
                };
                
                // Add random floating animation
                object.floatingSpeed = {
                    x: (Math.random() - 0.5) * 0.005,
                    y: (Math.random() - 0.5) * 0.005
                };
                
                object.initialPosition = {
                    x: object.position.x,
                    y: object.position.y
                };
                
                object.floatingOffset = {
                    x: 0,
                    y: 0
                };
                
                object.floatingFactor = {
                    x: Math.random() * Math.PI * 2,
                    y: Math.random() * Math.PI * 2
                };
                
                scene.add(object);
                objects.push(object);
            }
            
            // Animation loop
            function animate() {
                requestAnimationFrame(animate);
                
                // Update object rotations and positions
                objects.forEach(object => {
                    object.rotation.x += object.rotationSpeed.x;
                    object.rotation.y += object.rotationSpeed.y;
                    object.rotation.z += object.rotationSpeed.z;
                    
                    // Update floating animation
                    object.floatingFactor.x += 0.01;
                    object.floatingFactor.y += 0.01;
                    
                    object.floatingOffset.x = Math.sin(object.floatingFactor.x) * 0.5;
                    object.floatingOffset.y = Math.sin(object.floatingFactor.y) * 0.5;
                    
                    object.position.x = object.initialPosition.x + object.floatingOffset.x;
                    object.position.y = object.initialPosition.y + object.floatingOffset.y;
                });
                
                renderer.render(scene, camera);
            }
            
            // Handle window resize
            window.addEventListener('resize', () => {
                camera.aspect = window.innerWidth / window.innerHeight;
                camera.updateProjectionMatrix();
                renderer.setSize(window.innerWidth, window.innerHeight);
            });
            
            animate();
        }
        
        // Initialize Chart.js
        function initChart() {
            const ctx = document.getElementById('learning-improvement-chart').getContext('2d');
            const chart = new Chart(ctx, {
                type: 'bar',
                data: {
                    labels: ['Math', 'Reading', 'Science', 'Problem Solving', 'Memory'],
                    datasets: [{
                        label: 'Average Improvement After 3 Months',
                        data: [42, 38, 35, 45, 33],
                        backgroundColor: 'rgba(255, 255, 255, 0.2)',
                        borderColor: 'rgba(255, 255, 255, 0.8)',
                        borderWidth: 2
                    }]
                },
                options: {
                    responsive: true,
                    plugins: {
                        legend: {
                            labels: {
                                color: 'rgba(255, 255, 255, 0.8)'
                            }
                        }
                    },
                    scales: {
                        y: {
                            beginAtZero: true,
                            max: 50,
                            ticks: {
                                color: 'rgba(255, 255, 255, 0.8)'
                            },
                            grid: {
                                color: 'rgba(255, 255, 255, 0.1)'
                            }
                        },
                        x: {
                            ticks: {
                                color: 'rgba(255, 255, 255, 0.8)'
                            },
                            grid: {
                                color: 'rgba(255, 255, 255, 0.1)'
                            }
                        }
                    }
                }
            });
        }
        
        // Animate stats counters
        function animateCounter(elementId, targetValue) {
            const element = document.getElementById(elementId);
            const duration = 2000; // 2 seconds
            const frameDuration = 1000 / 60; // 60fps
            const totalFrames = Math.round(duration / frameDuration);
            let frame = 0;
            
            const initialValue = 0;
            const valueIncrement = targetValue / totalFrames;
            
            const counter = setInterval(() => {
                frame++;
                const currentValue = Math.round(initialValue + valueIncrement * frame);
                
                // Format the value with commas
                element.textContent = currentValue.toLocaleString() + (targetValue >= 100 ? '+' : '');
                
                if (frame === totalFrames) {
                    clearInterval(counter);
                }
            }, frameDuration);
        }
        
        // Initialize GSAP animations
        function initGSAPAnimations() {
            gsap.registerPlugin(ScrollTrigger);
            
            // Navbar scroll animation
            gsap.to('nav', {
                scrollTrigger: {
                    start: 'top -80',
                    end: '+=1',
                    toggleClass: { className: 'scrolled', targets: 'nav' }
                }
            });
            
            // Feature cards stagger animation
            gsap.from('.feature-card', {
                scrollTrigger: {
                    trigger: '.features',
                    start: 'top 80%'
                },
                y: 50,
                opacity: 0,
                duration: 0.8,
                stagger: 0.15
            });
            
            // App cards stagger animation
            gsap.from('.app-card', {
                scrollTrigger: {
                    trigger: '.app-grid',
                    start: 'top 80%'
                },
                y: 50,
                opacity: 0,
                duration: 0.8,
                stagger: 0.15
            });
            
            // Testimonial cards animation
            gsap.from('.testimonial-card', {
                scrollTrigger: {
                    trigger: '.testimonial-grid',
                    start: 'top 80%'
                },
                y: 30,
                opacity: 0,
                duration: 0.8,
                stagger: 0.15
            });
            
            // Stats animation
            gsap.from('.stat-item', {
                scrollTrigger: {
                    trigger: '.stats-grid',
                    start: 'top 80%',
                    onEnter: () => {
                        // Start counter animations when stats section is visible
                        animateCounter('stat-apps', 75);
                        animateCounter('stat-students', 185000);
                        animateCounter('stat-educators', 12000);
                        animateCounter('stat-countries', 45);
                    }
                },
                y: 30,
                opacity: 0,
                duration: 0.8,
                stagger: 0.15
            });
            
            // CTA animation
            gsap.from('.cta-content', {
                scrollTrigger: {
                    trigger: '.cta',
                    start: 'top 80%'
                },
                y: 30,
                opacity: 0,
                duration: 1
            });
        }
        
        // Mobile navigation toggle
        function initMobileNav() {
            const hamburger = document.getElementById('hamburger');
            const mobileNav = document.getElementById('mobile-nav');
            const mobileNavClose = document.getElementById('mobile-nav-close');
            const overlay = document.getElementById('overlay');
            const mobileNavLinks = document.querySelectorAll('.mobile-nav-link');
            
            hamburger.addEventListener('click', () => {
                mobileNav.classList.add('active');
                overlay.classList.add('active');
                document.body.style.overflow = 'hidden';
            });
            
            const closeNav = () => {
                mobileNav.classList.remove('active');
                overlay.classList.remove('active');
                document.body.style.overflow = '';
            };
            
            mobileNavClose.addEventListener('click', closeNav);
            overlay.addEventListener('click', closeNav);
            
            mobileNavLinks.forEach(link => {
                link.addEventListener('click', closeNav);
            });
        }
        
        // Sound effects for interactions
        function initSoundEffects() {
            const sounds = {
                hover: new Howl({
                    src: ['data:audio/mp3;base64,