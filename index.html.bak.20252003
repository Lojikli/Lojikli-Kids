        // Initialize navigation functionality
        function initNavigation() {
            const hamburger = document.getElementById('hamburger');
            const mobileNav = document.getElementById('mobile-nav');
            const mobileClose = document.getElementById('mobile-close');
            const overlay = document.getElementById('overlay');
            const mobileLinks = document.querySelectorAll('.mobile-link');
            
            // Toggle mobile navigation
            hamburger.addEventListener('click', function() {
                mobileNav.classList.add('active');
                overlay.classList.add('active');
                document.body.style.overflow = 'hidden';
            });
            
            // Close mobile navigation
            function closeNav() {
                mobileNav.classList.remove('active');
                overlay.classList.remove('active');
                document.body.style.overflow = '';
            }
            
            mobileClose.addEventListener('click', closeNav);
            overlay.addEventListener('click', closeNav);
            
            // Close navigation when clicking on mobile links
            mobileLinks.forEach(link => {
                link.addEventListener('click', closeNav);
            });
            
            // Smooth scroll for navigation links
            const links = document.querySelectorAll('a[href^="#"]');
            links.forEach(link => {
                link.addEventListener('click', function(e) {
                    const href = this.getAttribute('href');
                    
                    if (href === '#') return;
                    
                    e.preventDefault();
                    
                    const offsetTop = document.querySelector(href).offsetTop - 100;
                    
                    scroll({
                        top: offsetTop,
                        behavior: 'smooth'
                    });
                });
            });
            
            // Change nav background on scroll
            window.addEventListener('scroll', function() {
                const nav = document.querySelector('nav');
                if (window.scrollY > 50) {
                    nav.style.padding = '8px 0';
                    nav.style.boxShadow = 'var(--shadow-lg)';
                } else {
                    nav.style.padding = 'var(--spacing-md) 0';
                    nav.style.boxShadow = 'var(--shadow-md)';
                }
            });
        }
        
        // Function to create a nice name from filename
        function getNiceName(filename) {
            // Remove file extension
            let name = filename.replace(".html", "");
            
            // Remove version numbers
            name = name.replace(/v\d+(\.\d+)?/ig, "").replace(/V\d+(\.\d+)?/ig, "");
            
            // Replace hyphens, underscores and dots with spaces
            name = name.replace(/[-_.]/g, " ");
            
            // Capitalize first letter of each word
            name = name.split(" ").map(word => {
                if (word.length > 0) {
                    return word[0].toUpperCase() + word.slice(1);
                }
                return word;
            }).join(" ");
            
            // Trim any extra spaces
            name = name.trim();
            
            return name;
        }
        
        // Function to get icon for app based on filename
        function getIconForApp(filename) {
            const lowerName = filename.toLowerCase();
            
            if (lowerName.includes("math") || lowerName.includes("multiply") || 
                lowerName.includes("division") || lowerName.includes("fraction") || 
                lowerName.includes("algebra") || lowerName.includes("calculus") || 
                lowerName.includes("factor")) {
                return "fas fa-calculator";
            } else if (lowerName.includes("music") || lowerName.includes("piano") || 
                    lowerName.includes("midi") || lowerName.includes("note") || 
                    lowerName.includes("fifths")) {
                return "fas fa-music";
            } else if (lowerName.includes("country") || lowerName.includes("geo") || 
                    lowerName.includes("globe")) {
                return "fas fa-globe-americas";
            } else if (lowerName.includes("game") || lowerName.includes("battleship") || 
                    lowerName.includes("pacman") || lowerName.includes("shooter") || 
                    lowerName.includes("connect4") || lowerName.includes("soduku")) {
                return "fas fa-gamepad";
            } else if (lowerName.includes("read") || lowerName.includes("writing") || 
                    lowerName.includes("word")) {
                return "fas fa-book";
            } else if (lowerName.includes("timer")) {
                return "fas fa-clock";
            } else if (lowerName.includes("neural") || lowerName.includes("ai")) {
                return "fas fa-brain";
            } else {
                return "fas fa-puzzle-piece"; // Default icon
            }
        }
        
        // Function to get categories for an app based on filename
        function getCategoriesForApp(filename) {
            const lowerName = filename.toLowerCase();
            const categories = [];
            
            // Add categories based on keywords in the filename
            if (lowerName.includes("math") || lowerName.includes("multiply") || 
                lowerName.includes("division") || lowerName.includes("fraction") || 
                lowerName.includes("algebra") || lowerName.includes("calculus") || 
                lowerName.includes("factor") || lowerName.includes("geometry") ||
                lowerName.includes("number") || lowerName.includes("pemdas") ||
                lowerName.includes("subtract") || lowerName.includes("scientific") ||
                lowerName.includes("zero") || lowerName.includes("plot") ||
                lowerName.includes("triang") || lowerName.includes("trig") ||
                lowerName.includes("decimal") || lowerName.includes("statistics")) {
                categories.push("math");
            }
            
            if (lowerName.includes("music") || lowerName.includes("piano") || 
                lowerName.includes("midi") || lowerName.includes("note") || 
                lowerName.includes("fifths") || lowerName.includes("harmonic")) {
                categories.push("music");
            }
            
            if (lowerName.includes("country") || lowerName.includes("geo") || 
                lowerName.includes("globe")) {
                categories.push("geography");
            }
            
            if (lowerName.includes("reading") || lowerName.includes("writing") || 
                lowerName.includes("word")) {
                categories.push("language");
            }
            
            if (lowerName.includes("game") || lowerName.includes("battleship") || 
                lowerName.includes("pacman") || lowerName.includes("shooter") || 
                lowerName.includes("connect4") || lowerName.includes("adventure") ||
                lowerName.includes("soduku") || lowerName.includes("bubble") ||
                lowerName.includes("asteroid") || lowerName.includes("snake") ||
                lowerName.includes("solitaire") || lowerName.includes("trouble") ||
                lowerName.includes("lander") || lowerName.includes("mahjong")) {
                categories.push("games");
            }
            
            // Add "all" category to every app
            categories.push("all");
            
            return categories;
        }
        
        // Create buttons for each file
        function createAppButtons() {
            const toddlerGrid = document.getElementById('toddlerGrid');
            const elementaryGrid = document.getElementById('elementaryGrid');
            
            // Function to create a button for a file
            function createButton(filename, parentFolder) {
                const niceName = getNiceName(filename);
                const iconClass = getIconForApp(filename);
                const categories = getCategoriesForApp(filename);
                
                const button = document.createElement('a');
                button.href = `${parentFolder}/${filename}`;
                button.className = 'app-button';
                button.dataset.categories = categories.join(" ");
                
                const buttonHTML = `
                    <div class="app-image">
                        <i class="${iconClass}"></i>
                    </div>
                    <div class="app-content">
                        <h3 class="app-title">${niceName}</h3>
                        <div class="app-meta">
                            <span class="app-category-tag">${categories[0] !== "all" ? categories[0] : "interactive"}</span>
                        </div>
                    </div>
                `;
                
                button.innerHTML = buttonHTML;
                
                return button;
            }
            
            // Create buttons for toddler apps
            repository["2 yr old"].forEach(filename => {
                const button = createButton(filename, "2 yr old");
                toddlerGrid.appendChild(button);
            });
            
            // Create buttons for elementary apps
            repository["5 yr old"].forEach(filename => {
                const button = createButton(filename, "5 yr old");
                elementaryGrid.appendChild(button);
            });
        }
        
        // Handle category filtering
        function setupCategoryFiltering() {
            const categories = document.querySelectorAll('.category');
            const buttons = document.querySelectorAll('.app-button');
            
            categories.forEach(category => {
                category.addEventListener('click', () => {
                    // Remove active class from all categories
                    categories.forEach(cat => cat.classList.remove('active'));
                    
                    // Add active class to clicked category
                    category.classList.add('active');
                    
                    const selectedCategory = category.dataset.category;
                    
                    // Show/hide buttons based on selected category
                    buttons.forEach(button => {
                        const buttonCategories = button.dataset.categories.split(" ");
                        
                        if (selectedCategory === 'all' || buttonCategories.includes(selectedCategory)) {
                            button.style.display = 'flex';
                            if (typeof anime !== 'undefined') {
                                anime({
                                    targets: button,
                                    scale: [0.9, 1],
                                    opacity: [0, 1],
                                    easing: 'easeOutElastic(1, .6)',
                                    duration: 800,
                                    delay: anime.stagger(50)
                                });
                            }
                        } else {
                            button.style.display = 'none';
                        }
                    });
                });
            });
        }
        
        // Handle search functionality
        function setupSearch() {
            const searchBox = document.getElementById('searchBox');
            const buttons = document.querySelectorAll('.app-button');
            
            searchBox.addEventListener('input', () => {
                const searchTerm = searchBox.value.toLowerCase();
                
                buttons.forEach(button => {
                    const buttonText = button.textContent.toLowerCase();
                    
                    if (buttonText.includes(searchTerm)) {
                        button.style.display = 'flex';
                        if (typeof anime !== 'undefined') {
                            anime({
                                targets: button,
                                scale: [0.95, 1],
                                opacity: [0.8, 1],
                                easing: 'easeOutElastic(1, .6)',
                                duration: 500,
                                delay: anime.stagger(30)
                            });
                        }
                    } else {
                        button.style.display = 'none';
                    }
                });
            });
        }
        
        // Animate elements on scroll
        function animateOnScroll() {
            if (typeof anime === 'undefined') return;
            
            // Function to check if element is in viewport
            function isInViewport(element) {
                const rect = element.getBoundingClientRect();
                return (
                    rect.top <= (window.innerHeight || document.documentElement.clientHeight) * 0.8 &&
                    rect.bottom >= 0
                );
            }
            
            // Elements to animate
            const elements = {
                features: document.querySelectorAll('.feature'),
                ageGroups: document.querySelectorAll('.age-group'),
                testimonials: document.querySelectorAll('.testimonial-card'),
                sectionTitles: document.querySelectorAll('.section-title')
            };
            
            // Set initial state (hidden)
            elements.features.forEach(el => {
                el.style.opacity = '0';
                el.style.transform = 'translateY(30px)';
            });
            
            elements.ageGroups.forEach(el => {
                el.style.opacity = '0';
                el.style.transform = 'translateY(30px)';
            });
            
            elements.testimonials.forEach(el => {
                el.style.opacity = '0';
                el.style.transform = 'translateY(30px)';
            });
            
            elements.sectionTitles.forEach(el => {
                el.style.opacity = '0';
                el.style.transform = 'translateY(20px)';
            });
            
// Function to animate elements
function animateElements() {
                // Animate features
                elements.features.forEach(el => {
                    if (isInViewport(el) && el.style.opacity === '0') {
                        anime({
                            targets: el,
                            translateY: [30, 0],
                            opacity: [0, 1],
                            easing: 'easeOutCubic',
                            duration: 800,
                            delay: anime.stagger(100)
                        });
                    }
                });
                
                // Animate age groups
                elements.ageGroups.forEach(el => {
                    if (isInViewport(el) && el.style.opacity === '0') {
                        anime({
                            targets: el,
                            translateY: [30, 0],
                            opacity: [0, 1],
                            easing: 'easeOutCubic',
                            duration: 800,
                            delay: anime.stagger(150)
                        });
                    }
                });
                
                // Animate testimonials
                elements.testimonials.forEach(el => {
                    if (isInViewport(el) && el.style.opacity === '0') {
                        anime({
                            targets: el,
                            translateY: [30, 0],
                            opacity: [0, 1],
                            easing: 'easeOutCubic',
                            duration: 800,
                            delay: anime.stagger(150)
                        });
                    }
                });
                
                // Animate section titles
                elements.sectionTitles.forEach(el => {
                    if (isInViewport(el) && el.style.opacity === '0') {
                        anime({
                            targets: el,
                            translateY: [20, 0],
                            opacity: [0, 1],
                            easing: 'easeOutCubic',
                            duration: 600
                        });
                    }
                });
            }
            
            // Initial check on page load
            animateElements();
            
            // Check on scroll
            window.addEventListener('scroll', animateElements);
        }
        
        // Initialize floating animations for decorative elements
        function initFloatingElements() {
            if (typeof anime === 'undefined') return;
            
            // Game elements animation
            const gameElements = document.querySelectorAll('.game-element');
            gameElements.forEach(el => {
                const randomX = (Math.random() - 0.5) * 20;
                const randomY = (Math.random() - 0.5) * 20;
                const randomRotation = (Math.random() - 0.5) * 30;
                
                anime({
                    targets: el,
                    translateX: [0, randomX],
                    translateY: [0, randomY],
                    rotate: [0, randomRotation],
                    easing: 'easeInOutSine',
                    duration: 5000 + Math.random() * 3000,
                    direction: 'alternate',
                    loop: true
                });
            });
            
            // Floating elements in hero section
            const floatingElements = document.querySelectorAll('.floating-element');
            floatingElements.forEach(el => {
                const randomX = (Math.random() - 0.5) * 20;
                const randomY = (Math.random() - 0.5) * 20;
                const randomRotation = (Math.random() - 0.5) * 15;
                
                anime({
                    targets: el,
                    translateX: [0, randomX],
                    translateY: [0, randomY],
                    rotate: [0, randomRotation],
                    easing: 'easeInOutSine',
                    duration: 4000 + Math.random() * 2000,
                    direction: 'alternate',
                    loop: true
                });
            });
            
            // Logo animation
            const logoLetters = document.querySelectorAll('.logo-letter');
            anime({
                targets: logoLetters,
                translateY: [-3, 3],
                easing: 'easeInOutSine',
                duration: 2000,
                direction: 'alternate',
                loop: true
            });
        }
        
        // Initialize testimonial slider
        function initTestimonialSlider() {
            if (typeof anime === 'undefined') return;
            
            const testimonialCards = document.querySelectorAll('.testimonial-card');
            const testimonialSlider = document.querySelector('.testimonial-slider');
            let currentIndex = 0;
            
            // Only setup auto-scrolling if there are multiple testimonials and on desktop
            if (testimonialCards.length > 1 && window.innerWidth > 768) {
                // Auto scroll every 5 seconds
                setInterval(() => {
                    currentIndex = (currentIndex + 1) % testimonialCards.length;
                    const scrollPosition = testimonialCards[currentIndex].offsetLeft - testimonialSlider.offsetLeft;
                    
                    anime({
                        targets: testimonialSlider,
                        scrollLeft: scrollPosition,
                        easing: 'easeInOutQuad',
                        duration: 1000
                    });
                }, 5000);
            }
        }
        
        // Button hover effects
        function setupButtonEffects() {
            if (typeof anime === 'undefined') return;
            
            const buttons = document.querySelectorAll('.button-primary, .button-secondary, .cta-button-primary, .cta-button-secondary');
            
            buttons.forEach(button => {
                button.addEventListener('mouseenter', () => {
                    anime({
                        targets: button,
                        scale: 1.05,
                        easing: 'easeOutElastic(1, .6)',
                        duration: 600
                    });
                });
                
                button.addEventListener('mouseleave', () => {
                    anime({
                        targets: button,
                        scale: 1,
                        easing: 'easeOutElastic(1, .6)',
                        duration: 600
                    });
                });
            });
        }
        
        // Add these function calls to the DOMContentLoaded event
        document.addEventListener('DOMContentLoaded', function() {
            // Existing initialization...
            
            // Add new animations if anime.js is available
            if (typeof anime !== 'undefined') {
                initFloatingElements();
                initTestimonialSlider();
                setupButtonEffects();
            }
        });
    </script>
</body>
</html>