        }
        
        // Initialize age group buttons
        function initAgeGroupButtons() {
            const toddlerButton = document.getElementById('toddler-button');
            const elementaryButton = document.getElementById('elementary-button');
            const upperButton = document.getElementById('upper-button');
            
            const allAppsGrid = document.getElementById('all-apps');
            const toddlerAppsGrid = document.getElementById('toddler-apps');
            const elementaryAppsGrid = document.getElementById('elementary-apps');
            const upperAppsGrid = document.getElementById('upper-apps');
            
            // Show toddler apps
            toddlerButton.addEventListener('click', function(e) {
                e.preventDefault();
                
                allAppsGrid.classList.add('hidden');
                elementaryAppsGrid.classList.add('hidden');
                upperAppsGrid.classList.add('hidden');
                toddlerAppsGrid.classList.remove('hidden');
                
                // Scroll to app section
                document.getElementById('apps').scrollIntoView({ behavior: 'smooth', block: 'start' });
                
                // Update active state of category filters
                resetCategoryFilters();
            });
            
            // Show elementary apps
            elementaryButton.addEventListener('click', function(e) {
                e.preventDefault();
                
                allAppsGrid.classList.add('hidden');
                toddlerAppsGrid.classList.add('hidden');
                upperAppsGrid.classList.add('hidden');
                elementaryAppsGrid.classList.remove('hidden');
                
                // Scroll to app section
                document.getElementById('apps').scrollIntoView({ behavior: 'smooth', block: 'start' });
                
                // Update active state of category filters
                resetCategoryFilters();
            });
            
            // Show upper elementary apps
            upperButton.addEventListener('click', function(e) {
                e.preventDefault();
                
                allAppsGrid.classList.add('hidden');
                toddlerAppsGrid.classList.add('hidden');
                elementaryAppsGrid.classList.add('hidden');
                upperAppsGrid.classList.remove('hidden');
                
                // Scroll to app section
                document.getElementById('apps').scrollIntoView({ behavior: 'smooth', block: 'start' });
                
                // Update active state of category filters
                resetCategoryFilters();
            });
        }
        
        // Initialize category filters
        function initCategoryFilter() {
            const categoryButtons = document.querySelectorAll('.category');
            
            categoryButtons.forEach(button => {
                button.addEventListener('click', function() {
                    // Update active state
                    categoryButtons.forEach(btn => btn.classList.remove('active'));
                    this.classList.add('active');
                    
                    const category = this.dataset.category;
                    
                    // Get the currently visible app grid
                    const visibleGrid = document.querySelector('.app-grid:not(.hidden)');
                    const appCards = visibleGrid.querySelectorAll('.app-card');
                    
                    // Filter the app cards
                    appCards.forEach(card => {
                        if (category === 'all' || card.dataset.category === category) {
                            card.style.display = 'block';
                        } else {
                            card.style.display = 'none';
                        }
                    });
                });
            });
        }
        
        // Reset category filters to "All Categories"
        function resetCategoryFilters() {
            const categoryButtons = document.querySelectorAll('.category');
            
            categoryButtons.forEach(btn => btn.classList.remove('active'));
            document.querySelector('.category[data-category="all"]').classList.add('active');
            
            // Show all app cards in the visible grid
            const visibleGrid = document.querySelector('.app-grid:not(.hidden)');
            const appCards = visibleGrid.querySelectorAll('.app-card');
            
            appCards.forEach(card => {
                card.style.display = 'block';
            });
        }
        
        // Initialize search functionality
        function initSearch() {
            const searchBox = document.getElementById('app-search');
            
            searchBox.addEventListener('input', function() {
                const searchTerm = this.value.toLowerCase();
                
                // Get the currently visible app grid
                const visibleGrid = document.querySelector('.app-grid:not(.hidden)');
                const appCards = visibleGrid.querySelectorAll('.app-card');
                
                // Filter app cards based on search term
                appCards.forEach(card => {
                    const title = card.querySelector('.app-title').textContent.toLowerCase();
                    const description = card.querySelector('.app-description').textContent.toLowerCase();
                    
                    if (title.includes(searchTerm) || description.includes(searchTerm)) {
                        card.style.display = 'block';
                    } else {
                        card.style.display = 'none';
                    }
                });
            });
        }
        
