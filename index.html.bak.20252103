        // Initialize navigation functionality
        function initNavigation() {
            const hamburger = document.getElementById('hamburger');
            const mobileNav = document.getElementById('mobile-nav');
            const mobileClose = document.getElementById('mobile-close');
            const overlay = document.getElementById('overlay');
            const mobileLinks = document.querySelectorAll('.mobile-link');
            
            // Toggle mobile navigation
            hamburger.addEventListener('click', function() {
                mobileNav.classList.add('active');
                overlay.classList.add('active');
                document.body.style.overflow = 'hidden';
            });
            
            // Close mobile navigation
            function closeNav() {
                mobileNav.classList.remove('active');
                overlay.classList.remove('active');
                document.body.style.overflow = '';
            }
            
            mobileClose.addEventListener('click', closeNav);
            overlay.addEventListener('click', closeNav);
            
            // Close navigation when clicking on mobile links
            mobileLinks.forEach(link => {
                link.addEventListener('click', closeNav);
            });
            
            // Smooth scroll for navigation links
            const links = document.querySelectorAll('a[href^="#"]');
            links.forEach(link => {
                link.addEventListener('click', function(e) {
                    const href = this.getAttribute('href');
                    
                    if (href === '#') return;
                    
                    e.preventDefault();
                    
                    const offsetTop = document.querySelector(href).offsetTop - 80;
                    
                    scroll({
                        top: offsetTop,
                        behavior: 'smooth'
                    });
                });
            });
            
            // Change nav style on scroll
            window.addEventListener('scroll', function() {
                const nav = document.querySelector('nav');
                if (window.scrollY > 20) {
                    nav.style.boxShadow = var(--shadow-md);
                } else {
                    nav.style.boxShadow = var(--shadow-sm);
                }
            });
        }
        
        // Function to create a nice name from filename
        function getNiceName(filename) {
            // Remove file extension
            let name = filename.replace(".html", "");
            
            // Remove version numbers
            name = name.replace(/v\d+(\.\d+)?/ig, "").replace(/V\d+(\.\d+)?/ig, "");
            
            // Replace hyphens, underscores and dots with spaces
            name = name.replace(/[-_.]/g, " ");
            
            // Capitalize first letter of each word
            name = name.split(" ").map(word => {
                if (word.length > 0) {
                    return word[0].toUpperCase() + word.slice(1);
                }
                return word;
            }).join(" ");
            
            // Trim any extra spaces
            name = name.trim();
            
            return name;
        }
        
        // Function to get icon for app based on filename
        function getIconForApp(filename) {
            const lowerName = filename.toLowerCase();
            
            if (lowerName.includes("math") || lowerName.includes("multiply") || 
                lowerName.includes("division") || lowerName.includes("fraction") || 
                lowerName.includes("algebra") || lowerName.includes("calculus") || 
                lowerName.includes("factor")) {
                return "fas fa-calculator";
            } else if (lowerName.includes("music") || lowerName.includes("piano") || 
                    lowerName.includes("midi") || lowerName.includes("note") || 
                    lowerName.includes("fifths")) {
                return "fas fa-music";
            } else if (lowerName.includes("country") || lowerName.includes("geo") || 
                    lowerName.includes("globe")) {
                return "fas fa-globe-americas";
            } else if (lowerName.includes("game") || lowerName.includes("battleship") || 
                    lowerName.includes("pacman") || lowerName.includes("shooter") || 
                    lowerName.includes("connect4") || lowerName.includes("soduku")) {
                return "fas fa-gamepad";
            } else if (lowerName.includes("read") || lowerName.includes("writing") || 
                    lowerName.includes("word")) {
                return "fas fa-book";
            } else if (lowerName.includes("timer")) {
                return "fas fa-clock";
            } else if (lowerName.includes("neural") || lowerName.includes("ai")) {
                return "fas fa-brain";
            } else {
                return "fas fa-puzzle-piece"; // Default icon
            }
        }
        
        // Function to get categories for an app based on filename
        function getCategoriesForApp(filename) {
            const lowerName = filename.toLowerCase();
            const categories = [];
            
            // Add categories based on keywords in the filename
            if (lowerName.includes("math") || lowerName.includes("multiply") || 
                lowerName.includes("division") || lowerName.includes("fraction") || 
                lowerName.includes("algebra") || lowerName.includes("calculus") || 
                lowerName.includes("factor") || lowerName.includes("geometry") ||
                lowerName.includes("number") || lowerName.includes("pemdas") ||
                lowerName.includes("subtract") || lowerName.includes("scientific") ||
                lowerName.includes("zero") || lowerName.includes("plot") ||
                lowerName.includes("triang") || lowerName.includes("trig") ||
                lowerName.includes("decimal") || lowerName.includes("statistics")) {
                categories.push("math");
            }
            
            if (lowerName.includes("music") || lowerName.includes("piano") || 
                lowerName.includes("midi") || lowerName.includes("note") || 
                lowerName.includes("fifths") || lowerName.includes("harmonic")) {
                categories.push("music");
            }
            
            if (lowerName.includes("country") || lowerName.includes("geo") || 
                lowerName.includes("globe")) {
                categories.push("geography");
            }
            
            if (lowerName.includes("reading") || lowerName.includes("writing") || 
                lowerName.includes("word")) {
                categories.push("language");
            }
            
            if (lowerName.includes("game") || lowerName.includes("battleship") || 
                lowerName.includes("pacman") || lowerName.includes("shooter") || 
                lowerName.includes("connect4") || lowerName.includes("adventure") ||
                lowerName.includes("soduku") || lowerName.includes("bubble") ||
                lowerName.includes("asteroid") || lowerName.includes("snake") ||
                lowerName.includes("solitaire") || lowerName.includes("trouble") ||
                lowerName.includes("lander") || lowerName.includes("mahjong")) {
                categories.push("games");
            }
            
            // Add "all" category to every app
            categories.push("all");
            
            return categories;
        }
        
        // Create buttons for each file
        function createAppButtons() {
