                lowerName.includes("soduku") || lowerName.includes("bubble") ||
                lowerName.includes("asteroid") || lowerName.includes("snake") ||
                lowerName.includes("solitaire") || lowerName.includes("trouble") ||
                lowerName.includes("lander") || lowerName.includes("mahjong")) {
                categories.push("games");
            }
ECHO is off.
            // Add "all" category to every app
            categories.push("all");
ECHO is off.
            return categories;
        }
ECHO is off.
        // Create app cards for each file
        function createAppCards() {
            const toddlerGrid = document.getElementById('toddlerGrid');
            const elementaryGrid = document.getElementById('elementaryGrid');
ECHO is off.
            // Function to create an app card
            function createCard(filename, parentFolder) {
                const niceName = getNiceName(filename);
                const iconClass = getIconForApp(filename);
                const categories = getCategoriesForApp(filename);
ECHO is off.
                const card = document.createElement('a');
                card.href = `${parentFolder}/${filename}`;
                card.className = 'app-card';
                card.dataset.categories = categories.join(" ");
ECHO is off.
                const primaryCategory = categories[0]  "interactive";
ECHO is off.
                card.innerHTML = `
                    <div class="app-icon">
                        <i class="${iconClass}"></i>
                    </div>
                    <div class="app-info">
                        <div class="app-name">${niceName}</div>
                        <div class="app-category">${primaryCategory}</div>
                    </div>
                `;
ECHO is off.
                return card;
            }
ECHO is off.
            // Create cards for toddler apps
            repository["2 yr old"].forEach((filename, index) => {
                const card = createCard(filename, "2 yr old");
                card.style.animationDelay = `${index * 0.05}s`;
                toddlerGrid.appendChild(card);
            });
ECHO is off.
            // Create cards for elementary apps
            repository["5 yr old"].forEach((filename, index) => {
                const card = createCard(filename, "5 yr old");
                card.style.animationDelay = `${index * 0.02}s`;
                elementaryGrid.appendChild(card);
            });
        }
ECHO is off.
        // Handle category filtering
        function setupCategoryFiltering() {
            const categories = document.querySelectorAll('.category');
            const appCards = document.querySelectorAll('.app-card');
ECHO is off.
            categories.forEach(category => {
                category.addEventListener('click', () => {
                    // Update active state
                    categories.forEach(cat => cat.classList.remove('active'));
                    category.classList.add('active');
ECHO is off.
                    const selectedCategory = category.dataset.category;
ECHO is off.
                    // Filter the app cards
                    appCards.forEach(card => {
                        const cardCategories = card.dataset.categories.split(" ");
ECHO is off.
                        if (selectedCategory === 'all' || cardCategories.includes(selectedCategory)) {
                            card.style.display = 'flex';
                        } else {
                            card.style.display = 'none';
                        }
                    });
                });
            });
        }
ECHO is off.
        // Handle search functionality
        function setupSearch() {
            const searchBox = document.getElementById('searchBox');
            const appCards = document.querySelectorAll('.app-card');
ECHO is off.
            searchBox.addEventListener('input', () => {
                const searchTerm = searchBox.value.toLowerCase();
ECHO is off.
                appCards.forEach(card => {
                    const appName = card.querySelector('.app-name').textContent.toLowerCase();
ECHO is off.
                    if (appName.includes(searchTerm)) {
                        card.style.display = 'flex';
                    } else {
                        card.style.display = 'none';
                    }
                });
            });
        }
    </script>
</body>
</html>
